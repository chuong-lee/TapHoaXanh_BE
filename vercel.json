{
    "version": 2,
    "builds": [
        {
            "src": "src/main.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 1000
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.js"
        }
    ]
}